{
  "name": "ra2017",
  "version": "1.0.0",
  "description": "Front End Project for ReloAccess",
  "main": "lib/index.js",
  "scripts": {
    "prestart": "webpack",
    "start": "webpack-dev-server",
    "build": "webpack -p"
  },
  "keywords": [],
  "author": "Diogo Lopes",
  "license": "UNLICENSED",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "ie >= 11",
              "last 2 Chrome versions",
              "last 2 Firefox versions",
              "last 2 Safari versions",
              "last 2 ChromeAndroid versions",
              "last 2 Edge versions",
              "last 2 iOS versions"
            ]
          }
        }
      ],
      "react"
    ],
    "plugins": ["transform-object-rest-spread", "transform-object-assign"],
    "env": {
      "test": {
        "presets": ["es2015", "stage-2", "react"],
        "plugins": ["transform-es2015-modules-commonjs"]
      }
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/Layouts/",
      "<rootDir>/dist/",
      "<rootDir>/STYLES/"
    ],
    "snapshotSerializers": ["jest-serializer-html"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/tools/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/tools/assetsTransformer.js",
      "^__GLOBAL__(.*)$": "<rootDir>/src/global/$1",
      "^__SHARED__(.*)$": "<rootDir>/src/sharedJSX/$1"
    },
    "setupFiles": ["./tools/enzymeTestAdapterSetup.js"]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-react-css-modules": "^3.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "check-dependencies": "^1.0.1",

    "css-loader": "^0.28.4",

    "duplicate-package-checker-webpack-plugin": "^2.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.6.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5",
    "expect": "^22.0.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "jest": "^22.0.3",
    "jest-cli": "^22.0.3",
    "jest-serializer-html": "^5.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-css-modules": "^4.5.2",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.4.0",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9",
    "webpack-dev-server": "^2.9",
    "write-file-webpack-plugin": "^4.0.0"
  },
  "dependencies": {
    "es6-promise": "^4.1.1",

    "prop-types": "^15.5.10",
    "react": "^16.1",
    "react-dom": "^16.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  }
}
