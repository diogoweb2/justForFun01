{
  "extends": ["airbnb", "prettier", "plugin:jest/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/react-in-jsx-scope": 1,
    "react/jsx-wrap-multilines": 0,
    "no-debugger": 0,
    "import/no-unresolved": 0,
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "prefer-destructuring": [
      "error",
      {
        "object": false,
        "array": false
      }
    ],
    "react/no-multi-comp": 0,
    "react/forbid-prop-types": [
      1,
      {
        "forbid": ["any"]
      }
    ],
    "no-console": "off",
    "no-extend-native": 1
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "plugins": ["react", "jest"],
  "globals": {
    "DEV_ENV": false
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
